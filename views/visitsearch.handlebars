	<script>
		$(document).ready(function(){
			$("#tagsbtn").click(function(){
		    $("#tags").slideToggle();
		    $("#friends").slideUp();
			});
			$("#friendsbtn").click(function(){
		    $("#tags").slideUp();
		    $("#friends").slideToggle();
